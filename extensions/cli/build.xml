<?xml version="1.0"?>
<project name="dbtune-ml">
  <property file="build.properties"/>
  <path id="compile.classpath">
    <fileset dir="${lib.dir}" includes="*.jar"/>
    <fileset dir="../../${lib.dir}" includes="*.jar"/>
    <pathelement path="../../build/classes"/>
  </path>
  <target name="compile">
    <ant dir="../../" antfile="common.xml" target="scala.compile">
      <property name="base.dir" value="${basedir}" />
      <reference refid="compile.classpath" />
    </ant>
  </target>
  <target name="test.compile">
    <ant dir="../../" antfile="common.xml" target="scala.test.compile">
      <property name="base.dir" value="${basedir}" />
      <reference refid="compile.classpath" />
    </ant>
  </target>
  <target name="clean">
    <ant dir="../../" antfile="common.xml" target="clean">
      <property name="base.dir" value="${basedir}" />
      <reference refid="compile.classpath" />
    </ant>
  </target>
  <target name="test.one">
    <ant dir="../../" antfile="common.xml" target="test.one">
      <property name="base.dir" value="${basedir}" />
      <reference refid="compile.classpath" />
    </ant>
  </target>
</project>
